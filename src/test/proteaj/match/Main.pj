package match;

import dsl MatchDSL;
import dsl StringMatcherDSL;

public class Main {
  public static void main (String[] args) {
    String s = "bab" match {
      | "b" ~ "a" ~ rest => rest
      | "b" ~ rest       => rest
      | starts with "a"  => "starts with a"
      | x => x
    };
    System.out.println(s);
  }
}